;spectreS.envOpts    modelPath       string  "~/cadence/models/spectre /afs/eos/dist/cadence/local/models/spectre/public"


;--------------------------------------------------------------------------
; User Preference Defaults
;--------------------------------------------------------------------------
ui    showScrollBars  boolean t
ui    showOptionForms boolean t
ui    undoLevel       int     10

